[core]
	editor = vim
[color]
	ui = auto
	interactive = auto
[diff]
	renames = copies
[alias]
  tree = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  vtag = tag --sort=version:refname
[push]
	default = simple
[commit]
  gpgsign = true
