[core]
  editor = nvim
  autocrlf = input
  fileMode = false
[color]
  ui = auto
  interactive = auto
[diff]
  renames = copies
[alias]
  tree = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cd) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  vtag = tag --sort=version:refname
[push]
  default = simple
